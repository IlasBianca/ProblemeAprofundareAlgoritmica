#include <iostream>
using namespace std;
//problema 4/ probleme aprofundare c++
//Sa se scrie un program care genereaza sirul lui fibonacci. Generezi numere din sirul lui fibonacci pana cand se depaseste un numar n citit de la tastatura.
int main() {
    int n, fibo1, fibo2, sirFibo;
    fibo1=0;
    fibo2=1;
    cout<<fibo1<<endl;
    cout<<fibo2<<endl;
    n=320;
    while (fibo1 <=n || fibo2<=n){
        sirFibo=fibo1+fibo2;
        fibo1=sirFibo;
        fibo2= sirFibo+ fibo2;
        if (fibo1<=n || fibo2<=n){
            cout<<fibo1<<endl;
            cout<<fibo2<<endl;
        }

    }

    return 0;
}
