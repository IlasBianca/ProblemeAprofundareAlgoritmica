#include <iostream>
using namespace std;
//problema 10/ probleme aprofundare c++
//Calculeaza factorialul unui numar n introdus de la tastatura
int main() {
    int n, factorial=1;
    cin>>n;
    for (int i=1; i<=n; i++){
        factorial=factorial*i;
    }
    cout<<factorial<<endl;
    return 0;
}
