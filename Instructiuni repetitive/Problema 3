#include <iostream>
using namespace std;
//problema 3/ probleme aprofundare c++
//Sa se genereze triplete de numere pozitive care sunt mai mici decat un numar citit n, a caror suma este mai mica sau egala decat m.
int main() {
    int n, m;
    n=5;
    m=5;
    for (int a=0;a<=n; a++){
        for (int b=0; b<=n; b++){
            for (int c=0; c<=n; c++){
                if ((a+b+c)>=0 && (a+b+c)<=m){
                    cout<<"("<<a<<","<<b<<","<<c<<")"<<endl;
                }
            }

        }
    }
    return 0;
}
